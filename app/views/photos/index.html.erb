<h1>Listing photos</h1>

<table>
  <thead>
    <tr>
      <th>Image uid</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @photos.each do |photo| %>
      <tr>
        <td><%= link_to(image_tag(photo.image.thumb('64x64!').url, alt: nil), photo) if photo.image_stored? %></td>
        <td><%= link_to 'Show', photo %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

