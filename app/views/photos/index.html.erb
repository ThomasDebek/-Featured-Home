<h1>Galeria</h1>

<table>


  <!-- Pobawmy sie teraz widoczkiem -->


  <div id="photo">
    <table>
      <% @photos.each do |photo| %>
          <div class="col-md-4">
            <div class="thumbnail_new">
              <%= link_to(image_tag(photo.image.thumb('304x224!').url, alt: nil), photo_path(photo)) if photo.image_stored? %>
            </div>
          </div>
      <% end %>
    </table>


  </div>


</table>

<br>
